<Window x:Class="WPFApp_8_Button.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFApp_8_Button"
        mc:Ignorable="d"
        Title="MainWindow" Width="800">
    <StackPanel>
        <Button x:Name="Button1" Height="40" Margin="10" Content="byte" Click="Button1_Click"/>
        <Button x:Name="Button2" Height="40" Margin="10" Content="sbyte"/>
        <Button x:Name="Button3" Height="40" Margin="10" AutomationProperties.ItemStatus="asd" Content="ushort"/>
        <Button x:Name="Button4" Height="40" Margin="10" Content="short"/>
        <Button x:Name="Button5" Height="40" Margin="10" Content="uint" Click="Button5_Click"/>
        <Button x:Name="Button6" Height="40" Margin="10" Content="int"/>
        <Button x:Name="Button7" Height="40" Margin="10" Content="ulong"/>
        <Button x:Name="Button8" Height="40" Margin="10" Content="long" Click="Button_Click"/>
    </StackPanel>
</Window>
